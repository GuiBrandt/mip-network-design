    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch - Polynomial",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/mo420-network-design-1",
            "args": [
                "instances/mo420_network_design_1_10_100_20_80_1"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch - Branch-And-Cut",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/mo420-network-design-2",
            "args": [
                "instances/mo420_network_design_1_50_100_20_80_5"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}